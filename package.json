    "int64",
    "int8array",
    "integer",
    "long",
    "longlong",
    "signed",
    "uint64",
    "unsinged"
  ],
  "license": "MIT",
  "main": "int64-buffer.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/int64-buffer.git"
  },
  "scripts": {
    "coveralls": "./node_modules/.bin/nyc make test && ./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "fixpack": "fixpack",
    "test": "make test",
    "zuul-local": "./node_modules/.bin/zuul --open --local 4000 --ui mocha-bdd test/*.js"
  },
  "typings": "int64-buffer.d.ts"
